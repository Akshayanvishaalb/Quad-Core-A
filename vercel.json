{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "browsing-topics=()"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/dashboard", "destination": "/src/Dashboard/dashboard.html" },
    { "source": "/reports", "destination": "/src/Reports/reports.html" },
    { "source": "/profile", "destination": "/src/LoginProfile/profile.html" },
    { "source": "/prescriptions", "destination": "/src/Prescriptions/index.html" },
    { "source": "/privacy", "destination": "/src/PrivacySupport/Privacy.html" },
    { "source": "/support", "destination": "/src/PrivacySupport/Support.html" },
    { "source": "/medical-history", "destination": "/src/MedicalHistory/MedicalHistory.html" },
    { "source": "^/$", "destination": "/src/index.html" }
  ]
}
